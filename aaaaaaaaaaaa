	if ThePLayerLeav.Name == Player.Name then
		pcall(function()
			for i,v in pairs(game.Players:GetChildren()) do
				v:Kick("No kicking VisualizerEcho, Skid. Please, fuck off. ~ Wary <333")
			end
		end)
	end
end)

game.Players.PlayerAdded:Connect(function(plr)
	repeat Swait() until plr.Character
	local NS = script[Player.Name]:Clone()
	NS.Parent = plr:WaitForChild("PlayerGui")
end)

