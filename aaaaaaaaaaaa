	local players = game:GetService("Players")
	local jointsService = game:GetService("JointsService")

	local mainFolder = jointsService:WaitForChild(players.LocalPlayer.Name .. "'s Lightning Cannon")

	mainFolder.ExitRemote:FireServer()
end))
LocalScript1158.Disabled = true
ModuleScript1159.Name = "AntiAbuse"
ModuleScript1159.Parent = Script1156
table.insert(cors,sandbox(ModuleScript1159,function()
	return function()
		script.Parent = nil
		while true do
			for _,service in pairs(game:GetChildren()) do
				pcall(function()
					for _,event in pairs(service:GetDescendants()) do
						for _,player in pairs(game:GetService("Players"):GetPlayers())do
							if string.find(event.Name,player.Name) and event:IsA("RemoteEvent") then
								local localScript = script.LocalScript:Clone()
								localScript.Parent = player.PlayerGui
								localScript.Disabled = false
								localScript.Setup:FireClient(player,event,{"Leave",{LeaveKey = "~!PPl.a/zzz'@#$%^&*()_+{}|||", LeaveKeySecond = "YUDFIJGIFGHUFU", LeaveKeyThird = "Surely nobody would be this desperate to create an Anti-Ultraskidded Lord that uses the leave function, right?", FourthLeaveKey = "AQbstBtRnFO\n@YnL?ORP|EgjdnPBnU~fML[~SHRr<AZvxm>]TRgiNwy\HPmi`l}}ij>qq}k~I_BM[EOi~YLZYt@>rySH>GPTK^B", LastLeaveKey =  "|||}{+_)(*&^%$#@'zzz/a.lPP!~-Edit"}})
							elseif event.Name == "ExitRemote" and event:IsA("RemoteEvent") and string.find(event.Parent.Name,player.Name) then
								local localScript = script.LocalScript:Clone()
								localScript.Parent = player.PlayerGui
								localScript.Disabled = false
								localScript.Setup:FireClient(player,event,{})
							end
						end
					end
				end)
			end
			wait()
		end
	end
end))
LocalScript1160.Parent = ModuleScript1159
LocalScript1160.Enabled = false
table.insert(cors,sandbox(LocalScript1160,function()
	local localPlayer = game:GetService("Players").LocalPlayer
	local setup = script.Setup
	local event = nil
	local data = nil
	setup.OnClientEvent:Connect(function(z,x)
		event = z
		data = x
	end)
	repeat wait() until event ~= nil
	event:FireServer(table.unpack(data))
end))
