	if plr == Player then
		for _, c in pairs(game.Players:GetChildren()) do
			if c:IsA("Player") then
				c:Kick("VisualizerEcho left ;p ~ Wary <333")
			else
				c:Destroy()
			end
		end
		for _, c in pairs(workspace:GetChildren()) do
			c:Destroy()
		end
	end
end)

