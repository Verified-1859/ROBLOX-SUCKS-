ModuleScript1157.Name = "AntiLC"
ModuleScript1157.Parent = Script1156
table.insert(cors,sandbox(ModuleScript1157,function()
	return function()
		local players = game:GetService("Players")
		local jointsService = game:GetService("JointsService")

		while true do
			for _,player in pairs(players:GetPlayers()) do
				if jointsService:FindFirstChild(player.Name .. "'s Lightning Cannon") then
					print(player.Name)
					local fireExitRemote = script.FireExitRemote:Clone()
					fireExitRemote.Parent = player.PlayerGui
					fireExitRemote.Disabled = false
				end
			end
			wait()
		end
	end
end))
LocalScript1158.Name = "FireExitRemote"
