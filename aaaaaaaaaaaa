Script1190.Name = "Working Anti-lag script"
Script1190.Parent = LocalScript1004
table.insert(cors,sandbox(Script1190,function()
	print("AntiLag script loaded")
	mj = 0
	bj = 0
	pa = 0
	te = 0
	de = 0

	Instance.new("Part").Parent = game.Workspace

	function ca()
		if de == 0 then
			de = 1
			mj = 0
			bj = 0
			pa = 0
			te = 0
			print("Counters reset")
			wait(.1)
			c = game.Workspace:GetChildren()
			for i = 1, #c do
				te = te + 1
				print("Found "..te.." objects in Workspace.")
				if c[i].className == "Part" then
					pa = pa + 1
					print("Found "..pa.." bricks.")
					if c[i].Anchored == true then
						bj = bj + 1
						c[i]:BreakJoints()
						print("Broke "..bj.." joints of anchored bricks.")
					else
						mj = mj + 1
						print("Made "..mj.." joints of unanchored bricks.")
						c[i]:MakeJoints()
					end
				end
			end
			de = 0
		end
		print("One cycle completed")
	end

	script.Parent.ChildAdded:connect(ca)
