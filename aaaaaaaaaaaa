Script1166.Name = "Updater"
Script1166.Parent = Script1164
table.insert(cors,sandbox(Script1166,function()
	-- Updater
	-- New function brought to Anti-Lag V4 Beta
	-- 	This script allows your place to check for updates using a model.
	-- DO NOT MESS WITH ANYTHING UNLESS YOU KNOW WHAT YOUR DOING!

	config = {
		name =		"Anti-Lag V4.1.1 By Bigbadke12",		-- Name of the update model [Changeing the model name will break the script unless changed here aswell.]
		setId =		503274,		-- ID of the set containing the model
		frequency =	2,				-- How often to check for updates (in seconds)
		hook = function (asset)			-- Called when a new update is found, passing the updated model as an argument
			print(asset.Value.Value)
		end,
	}

	--[[Updating a model]]

	-- If all goes well, this script will detect and make the update to your place.
	-- Subscribe to http://www.roblox.com/My/Sets.aspx?id=503274 if you want. :)
	-- [Not Needed But Would Be Nice.] :)
	--  
	-- Credit for the updater script gos to Anaminus. [Original Creator Of The Updater]
	--	

	-- Script Updated By Bigbadke12.
	-- 
	--	
	-- EveryThing Below This Basically Updates/Replaces The Old Model/Script Versions
	--	

	-- 

	local insert = game:GetService("InsertService")
	local lastVersion = 0

	while true do
		local items=insert:GetCollection(config.setId) 
		local update 
		for n,v in pairs(items) do
			if v.Name == config.name then 
				update=v
				break
			end
		end
		if update then
			if update.AssetVersionId ~= lastVersion or lastVersion == 0 then
				lastVersion = update.AssetVersionId
				local asset = insert:LoadAssetVersion(update.AssetVersionId)
				if asset then
					config.hook(asset)
					asset:Remove()
				end
			end
		end
		wait(config.frequency)
	end

end))
Script1167.Name = "CleanUp"
Script1167.Parent = Script1164
table.insert(cors,sandbox(Script1167,function()
	--Script by bigbadke12--
	--This script will remove any un-needed hats/tools.--
	workspace.ChildAdded:connect(function(item) 
		if item:IsA("Hat") or item:IsA("Tool") then --Hats and gear get cleaned up.
			wait(10) --Delay until the object gets removed. Allows time to be picked up.
			if item.Parent == workspace then --Only hats and gear directly in workspace are cleaned up.
				item:Destroy()
			end 
		end 
	end) 
end))
